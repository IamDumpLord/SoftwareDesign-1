
public class Stock
{
	int total;
	Product[] prodArray;
	
	total = 0;
	
	
	public Stock(Product[] prodArray)
	{
		prodArray = new Product[100];
		for (int i=0; i<prodArray.length; i++)
		{
			prodArray[i] = new Product();
		}
	}
	public void printArray()
	{
		System.out.println("Index\tProduct");
		for (int i=0; i<prodArray.length; i++)
		{
			System.out.println(prodArray[i]);
		}
	}
	
	
	
	
	
	
}
